{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch strutcode.dev",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "https://2dmmo.strutcode.dev",
      "webRoot": "${workspaceFolder}/final/client/",
      "pathMappings": [
        {
          "url": "webpack:///src",
          "path": "${workspaceFolder}/src"
        }
      ]
    },
    {
      "name": "Attach",
      "type": "firefox",
      "request": "attach"
    },
    {
      "name": "Launch WebExtension",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "addonPath": "${workspaceFolder}"
    }
  ]
}
